{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "OSVersion": {
      "value": {
        "Image": "18.04-LTS",
        "Publisher": "Canonical",
        "Offer": "UbuntuServer"
      }
    },
    "vmSize": {
      "value": "Standard_B1s"
    },
    "sizeOfEachDataDiskInGB": {
      "value": "4095"
    },
    "adminUsername": {
      "value": "sysadmin"
    },
    "adminPasswordOrKey": {
      "value": "ssh-rsa  localhost"
    },
    "commandToExecute": {
      "value": "cat /etc/os-release"
    },
    "autoShutdownStatus": {
      "value": "Enabled"
    },
    "autoShutdownTime": {
      "value": "14:00"
    },
    "autoShutdownTimeZone": {
      "value": "UTC"
    },
    "autoShutdownNotificationStatus": {
      "value": "Disabled"
    },
    "autoShutdownNotificationLocale": {
      "value": "en"
    },
    "scenarioPrefix": {
        "value": "lab"
    }
  }
}